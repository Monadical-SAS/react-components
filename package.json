{
  "name": "monadical-react-components",
  "version": "0.0.6",
  "description": "Private, in-house react components.",
  "author": "Nick Sweeting <git@nicksweeting.com>",
  "license": "MIT",
  "homepage": "https://github.com/Monadical-SAS/react-components/",
  "bugs": {
    "url": "https://github.com/Monadical-SAS/react-components/issues",
    "email": "react-components-bugs@monadical.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Monadical-SAS/react-components.git"
  },
  "main": "node/main.js",
  "dependencies": {
    "react": "^15.6.1"
  },
  "devDependencies": {
    "aliasify": "^2.1.0",
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-plugin-syntax-function-bind": "^6.13.0",
    "babel-plugin-transform-flow-strip-types": "^6.14.0",
    "babel-plugin-transform-function-bind": "^6.8.0",
    "babel-plugin-transform-react-constant-elements": "^6.9.1",
    "babel-plugin-transform-react-inline-elements": "^6.8.0",
    "babel-plugin-transform-runtime": "^6.12.0",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-2": "^6.13.0",
    "babel-runtime": "^6.11.6",
    "babelify": "^7.3.0",
    "browserify": "^14.4.0",
    "eslint": "^3.19.0",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-react": "^7.0.1",
    "minifyify": "^7.3.5",
    "react": "^15.6.1",
    "react-bootstrap": "^0.31.0",
    "react-dom": "^15.5.4",
    "watchify": "^3.9.0"
  },
  "browserify": {
    "transform": [
      "babelify",
      "aliasify"
    ]
  },
  "babel": {
    "presets": [
      "react",
      "es2015",
      "stage-2"
    ],
    "plugins": [
      "syntax-function-bind",
      "transform-function-bind",
      "transform-react-inline-elements",
      "transform-react-constant-elements",
      "transform-runtime",
      "transform-flow-strip-types"
    ]
  },
  "scripts": {
    "build": "./scripts/build.sh"
  }
}
